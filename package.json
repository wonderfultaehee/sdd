   "scripts": {
      "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "coverage": "jest --coverage",
      "build": "nodemon --watch '*.ts' --exec 'ts-node' src/app/index.ts",
      "start": "ts-node src/app/index.ts",
      "api-docs": "swagger-cli bundle ./src/swagger/openapi.yaml --outfile build/swagger.yaml --type yaml",
      "seed:user": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n seedconfig.ts --seed CreateUser",
      "test": "jest --watch"
